@charset "utf-8";


html, body {
    margin: 0px;
    padding: 0px;
    background: url('images/body_bg.jpg') left top repeat;
    width: 100%;
    height: 100%;
    font-family: tahoma;
}

body {
    position: absolute;
}

.clear {
    clear: both;
    height: 0px;
    width: 100%;
    font-size: 0px;
}

img {
    border: none;
    margin: 0px;
    outline: none;
}

button::-moz-focus-inner,
	input[type="reset"]::-moz-focus-inner,
	input[type="button"]::-moz-focus-inner,
	input[type="submit"]::-moz-focus-inner,
	input[type="submit"]::-moz-focus-inner,
	input[type="file"] > input[type="button"]::-moz-focus-inner {/* */
	border: none;
}

a {
    font: normal normal 12px/12px tahoma;
    color: #676767;
    text-decoration: underline;
    outline: none;
}

a:hover {
    text-decoration: none;
}

h1 {
    display: block;
    margin: 0px 0px 0px 0px;
    padding: 0px 0px 0px 0px;
    text-align: left;
    font: normal normal 20px/20px tahoma;
    color: #000;
}

h2, h3 {
    display: block;
    margin: 0px 0px 0px 0px;
    padding: 0px 0px 0px 0px;
    text-align: left;
    font: normal normal 20px/20px tahoma;
    color: #000;
}

p {
    margin: 0px 0px 15px 0px;
    padding: 0px 0px 0px 0px;
    text-align: justify;
    font: normal normal 12px/24px tahoma;
    color: #676767;
}

ul {
    margin: 0px 0px 0px 0px;
    padding: 0px 0px 0px 0px;
    color: #676767;
    list-style: disc inside;
}

ul li {
    margin: 0px 0px 0px 0px;
    padding: 0px 0px 0px 0px;
    font: normal normal 12px/24px tahoma;
    color: #676767;
}

#header {
    position: relative;
    z-index: 12;
    background: -webkit-linear-gradient(#f5f5f5, #ebebeb);
    background: -o-linear-gradient(#f5f5f5, #ebebeb);
    background: -moz-linear-gradient(#f5f5f5, #ebebeb);
    background: linear-gradient(#f5f5f5, #ebebeb);
    box-shadow: 0px 0px 4px #999;
    width: 100% !important;
    min-width: 1170px !important;
}

