    margin: 0px auto 0px auto;
    /*height: 166px;*/
    overflow: hidden;
}

.viewed .viewport .container {
    width: 999999px;
    height: 166px;
}

.viewed .viewport .container .item {
    float: left;
    width: 126px;
    margin: 0px 25px 25px 0px;
    text-align: center;
}

.viewed .viewport .container .item a.pic {
    display: block;
    padding: 0px;
    width: 120px;
    height: 120px;
    margin: 0px 0px 5px 0px;
    -moz-border-radius: 50%;
    -webkit-border-radius: 50%;
    -khtml-border-radius: 50%;
    border-radius: 50%;
    border: solid 3px #989797;
    overflow: hidden;
}

.viewed .viewport .container .item a.pic:hover {
    border: solid 3px #fff;
}

.viewed .viewport .container .item .pic img {
    width: 120px;
    height: 120px;
    -moz-border-radius: 50%;
    -webkit-border-radius: 50%;
    -khtml-border-radius: 50%;
    border-radius: 50%;
}

.viewed .viewport .container .item a.title {
    font: normal normal 14px/25px tahoma;
    color: #eeeeee;
    text-align: center;
    text-decoration: none;
}

.viewed .viewport .container .item a.title:hover {
    text-decoration: underline;
}

.viewed .prev {
    display: block;
    position: absolute;
    z-index: 10;
    left: 50%;
