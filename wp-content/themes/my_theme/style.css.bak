.spoiler > input + label:after { 
content: "Подробнее"; /* Текст */
float: right;/* Выравнивание */
font-family: monospace;/* Семейство шрифта */
font-weight: bold;/* Насыщенность шрифта */
color: #fff;
 }
.spoiler > input:checked + label:after{ 
content: "-"; /* Символ + */
float: right; /* Выравнивание */
font-family: monospace; /* Семейство шрифта */
font-weight: bold; /* Насыщенность шрифта */
}
.spoiler > input {
display:none; /* Временно удаляет элемент */
}
.spoiler > input + label , 
.spoiler > .spoiler_body{
background:#32CD32; /* Цвет фона */
padding:5px 15px; /* Отступы от границы */
overflow:hidden; /* Отображается только область внутри элемента, остальное будет скрыто */
border: 2px solid #2dad2d;
width:100%; /* Ширина элемента */
box-sizing: border-box; /* Включают в себя значения полей и границ, но не отступов */
display: block; /* Элемент показывается как блочный */
}
.spoiler > input + label +
.spoiler_body{display:none;}
.spoiler > input:checked + label + 
.spoiler_body{display: block;}
.spoiler > .spoiler_body{
background: #FFFFFF; /* Цвет фона */
border: 2px solid #2dad2d; /* Параметры границ */
border-top: none;} /* Отключает верхнюю границу */
.spoiler > label:hover {
color:#FFFFFF; /* Цвет внутри элемента */
background:#FFA500; /* Цвет фона */
}

