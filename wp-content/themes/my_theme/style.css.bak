.thumbnail{
display: inline;
text-decoration: underline;
position: relative;
z-index: 0;
}
.thumbnail:hover{
text-decoration: none;
background-color: transparent;
z-index: 50;
}
.thumbnail span{ /*CSS for enlarged image*/
position: absolute;
background-color: #f0f8ff;
padding: 5px;
left: -1170px;
border: 1px solid white;
visibility: hidden;
color: black;
text-decoration: none;
border-radius: 4px 4px 4px 4px;
-moz-border-radius: 4px 4px 4px 4px;
-webkit-border-radius: 4px 4px 4px 4px;
}
.thumbnail:hover span{ /*CSS for enlarged image on hover*/
visibility: visible;
top: 20px;
left: -20px; /*position where enlarged image should offset horizontally */
}

.thumbnail > ins {
    position: relative;
}

.thumbnail > ins::before {
	content: "ФОТО";
	display: inline-block;
	width: 38px;
	height: 17.5px;
	background: #3EB87A;
	color: #fff;
	text-decoration: none;
	vertical-align: middle;
	font-weight: bold;
	filter: Shadow(Color=#00000, Direction=45, Strength=1);
	text-shadow: black 1px 1px;
	position: relative;
    top: 0;
	line-height: 1rem;
	font-size: 12px;
}

div.video .various::before {
    content: "ВИДЕО";
    display: inline-block;
    width: 44px;
    height: 17.5px;
    background: #7F4DA0;
    color: #fff;
    text-decoration: none;
    vertical-align: middle;
    font-weight: bold;
    filter: Shadow(Color=#00000, Direction=45, Strength=1);
    text-shadow: black 1px 1px;
    position: relative;
    top: 0;
    line-height: 1rem;
    font-size: 12px;
}

div.video:hover .various::before {
    content: "ЖМИ";
    display: inline-block;
    width: 44px;
    height: 17.5px;
    background: #7F4DA0;
    color: #fff;
    text-decoration: none;
    vertical-align: middle;
    font-weight: bold;
    filter: Shadow(Color=#00000, Direction=45, Strength=1);
    text-shadow: black 1px 1px;
    position: relative;
    top: 0;
    line-height: 1rem;
    font-size: 12px;
}

div.video .various {
    position: relative;
    cursor: pointer;
}

div.video {
    display: inline-block;
    text-align: center;
}

