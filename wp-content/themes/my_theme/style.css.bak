div.tematic ol, div.tematic ul {
    list-style: outside none none;
}

div.tematic {

  font-size: .9em;
  line-height: 1.2em;
  background: #e7e7e7;
  font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
  width: 10em;
  /*margin: 2em auto;*/
  background: #f6f6f6;
  -moz-border-radius: 0.4em / 0.4em;
  -webkit-border-radius: 0.4em 0.4em;
  border-radius: 0.4em / 0.4em;
  box-shadow: 0 0 4px rgba(0, 0, 0, 0.15);
}

div.tematic .user-avatar img {
  -moz-border-radius: 4em / 4em;
  -webkit-border-radius: 4em 4em;
  border-radius: 4em / 4em;
  float: left;
  margin-left: .618em;
  margin-top: 0.2em;
}

div.tematic .icon-reorder {
  padding: .8em 1em;
  color: white;
  border-radius: 0 .4em .4em 0;
  float: right;
}

div.tematic .drop {
  position: relative;
  cursor: pointer;
}

div.tematic .drop a {
  font-weight: 400;
  line-height: 2.5em;
  padding: 0 1em;
  color: #4d4d4d;
  text-decoration: none;
  white-space: nowrap;
}

div.tematic .triangle {
  width: 0;
  height: 0;
  position: absolute;
  border-left: .4em solid transparent;
  border-right: .4em solid transparent;
  border-top: .4em solid #FFB135;
  left: 89.5%;
  margin-left: -.4em;
  opacity: 0;
}

div.tematic .dropdownContain {
  width: 100%;
  position: absolute;
  z-index: 1;
  //left: 50%;
  top: -1000px;
  //margin-left: -6.5em;

}

div.tematic .dropOut a{
    display: block;
}

div.tematic .dropOut {
  width: auto;
  background: #f6f6f6;
  float: right;
  position: relative;
  margin-top: 0;
  opacity: 0;
  -moz-border-radius: 0.4em / 0.4em;
  -webkit-border-radius: 0.4em 0.4em;
  border-radius: 0.4em / 0.4em;
  box-shadow: 0 0 4px rgba(0, 0, 0, 0.15);
  border: 1px solid rgba(255, 255, 255, 0.75);
  overflow: hidden;
  -webkit-transition: all 0.1s fast-out;
  -moz-transition: all 0.1s fast-out;
  -ms-transition: all 0.1s fast-out;
  -o-transition: all 0.1s fast-out;
  transition: all 0.1s fast-out;
}

div.tematic .dropOut ul {
  padding: .3em 0;
  margin: 0;
}

div.tematic .dropOut li {
  text-align: left;
  width: 100%;
  /* padding: 0.6em 0; */
  color: gray;
  border-left: 3px solid #f7f7f7;
  -webkit-transition: all 0.1s ease-out;
  -moz-transition: all 0.1s ease-out;
  -ms-transition: all 0.1s ease-out;
  -o-transition: all 0.1s ease-out;
  transition: all 0.1s ease-out;
}
div.tematic .dropOut li span {
  color: #cccccc;
 /* margin: 0 1.5em 0 .8em; */
}
div.tematic .dropOut li:hover {
  border-color: #ffc568;
  color: #4d4d4d;
  background: #ededed;
}
div.tematic .dropOut li.drop:hover span {
  color: gray;
}

div.tematic ul li.drop:hover .triangle {
  opacity: 1;
}

div.tematic ul li.drop:hover .dropdownContain {
  top: 30px;
}

div.tematic ul li.drop:hover .dropOut {
  opacity: 1;
  margin-top: 20px;
}


div.tematic .orange-txt {
