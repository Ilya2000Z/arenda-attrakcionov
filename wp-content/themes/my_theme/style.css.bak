svg.eff-loader {
    overflow: visible;
    width: 100px;
    height: 150px;
}

svg.eff-loader g {
    animation: e1 2s linear infinite;
}

svg.eff-loader g:nth-child(2) {
    animation-delay: 0.5s;
}

svg.eff-loader g:nth-child(2) path {
    stroke-dashoffset: 1px;
    animation-delay: 0.5s;
    stroke-dasharray: 0px 158px;
}

svg.eff-loader path {
    fill: none;
    stroke-dasharray: 0 157px;
    stroke-dashoffset: 0;
    stroke: url(#gradient);
    stroke-width: 20px;
    stroke-linecap: round;
    animation: e2 2s cubic-bezier(0.8, 0, 0.2, 1) infinite;
}

@keyframes e1 {
    0% {
        transform: translateY(-50px);
    }
    100% {
        transform: translateY(50px);
    }
}

@keyframes e2 {
    0% {
        stroke-dasharray: 0 157px;
        stroke-dashoffset: 0;
    }
    50% {
        stroke-dasharray: 156px 157px;
        stroke-dashoffset: 0;
    }
    100% {
        stroke-dasharray: 156px 157px;
        stroke-dashoffset: -156px;
    }
}

